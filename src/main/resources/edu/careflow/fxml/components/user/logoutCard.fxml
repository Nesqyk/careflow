<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="411.0" prefWidth="460.0" spacing="32.0" style="-fx-background-color: white;" stylesheets="@../../../css/styles.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefHeight="73.0" prefWidth="266.0">
         <children>
            <Label text="Do you want to leave your account?">
               <font>
                  <Font name="☞Gilroy-SemiBold" size="24.0" />
               </font>
            </Label>
            <Label text=" Choose below to either log out or continue.">
               <font>
                  <Font name="☞Gilroy-Regular" size="18.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <HBox alignment="CENTER" prefHeight="59.0" prefWidth="266.0">
         <children>
            <Button fx:id="logoutBtn" mnemonicParsing="false" prefHeight="48.0" prefWidth="114.0" styleClass="view-button" stylesheets="@../../../css/styles.css" text="Logout">
               <font>
                  <Font name="☞Gilroy-Regular" size="16.0" />
               </font></Button>
            <Region prefHeight="100.0" prefWidth="32.0" />
            <Button fx:id="cancelBtn" mnemonicParsing="false" prefHeight="48.0" prefWidth="144.0" styleClass="view-button" stylesheets="@../../../css/styles.css" text="Cancel">
               <font>
                  <Font name="☞Gilroy-Regular" size="16.0" />
               </font></Button>
         </children>
      </HBox>
   </children>
</VBox>
